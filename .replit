modules = ["nodejs-20", "web", "bash"]
run = "npm run dev -- --host 0.0.0.0 --port 3000"

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3001
externalPort = 80

[[ports]]
localPort = 3002
externalPort = 3002

[[ports]]
localPort = 5000
externalPort = 5000

[workflows]

[[workflows.workflow]]
name = "Force Run Dev"
mode = "sequential"
author = 49781173

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install && npm run dev"

[userenv]

[userenv.shared]
